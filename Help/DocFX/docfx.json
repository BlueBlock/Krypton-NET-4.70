{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.sln"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "..\\/**"
          ],
          "src": "./../../Source/Krypton Components/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": true

    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "..\\/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "..\\/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "..\\/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Krypton Component Suite 2018\nBSD 3-Clause License\n© Component Factory Pty Ltd, 2006-2018, All rights reserved.",
      "_appFooter": "Modifications by Peter Wagner (aka Wagnerp) & Simon Coghlan (aka Smurf-IV) 2017 - 2018. All rights reserved. (https://github.com/Wagnerp/Krypton-NET-4.7)",
      "_appLogoPath": "logo.png",
      "_disableContribution": true
    },

    "dest": "..\\",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "docfx-tmpl/src"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}